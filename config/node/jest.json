{
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json"
    ],
    "collectCoverageFrom": [
        "**/src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
        "**/enum.ts",
        "**/interface.ts"
    ],
    "coverageReporters": [
        "json-summary",
        "lcov",
        "text",
        "html"
    ]
}